{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "CommonJS",
    "inlineSourceMap": true,
    "newLine": "LF",
    "declaration": true,
    "skipLibCheck": true,
    "target": "ES2024",
    "lib": ["ES2024", "DOM"],
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
